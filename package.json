{
  "name": "screenshot-puppeteer",
  "version": "1.0.0",
  "description": "server create screenshot by puppeteer-cluster in chrome",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:watch": "nodemon",
    "screenshot:prod": "node --unhandled-rejections=strict dist/main.js"
  },
  "keywords": [
    "screenshot",
    "node:12",
    "chrome-less",
    "puppeter",
    "puppeter-cluster"
  ],
  "author": "Andrii Ryzhyi",
  "license": "MIT",
  "dependencies": {
    "@types/puppeteer": "^5.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "puppeteer": "^12.0.1",
    "puppeteer-cluster": "^0.22.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.11",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  }
}
